{
  "name": "@stianlarsen/react-code-preview",
  "version": "1.2.0",
  "description": "A React component that provides tabbed navigation for viewing a live component preview and its source code separately.",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "sideEffects": [
    "*.css"
  ],
  "files": [
    "dist"
  ],
  "scripts": {
    "clean": "shx rm -rf dist",
    "build": "tsup",
    "prebuild": "npm run clean",
    "dev": "tsup --watch",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Stianlars1/react-code-preview.git"
  },
  "homepage": "https://react-code-preview.dev",
  "keywords": [
    "react",
    "code",
    "preview",
    "component",
    "live",
    "syntax",
    "highlighting",
    "shiki",
    "code-preview"
  ],
  "author": "Stian Larsen",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Stianlars1/react-code-preview/issues"
  },
  "peerDependencies": {
    "react": ">=18.0.0 <20.0.0",
    "react-dom": ">=18.0.0 <20.0.0"
  },
  "devDependencies": {
    "@types/react": "^18 || ^19",
    "@types/react-dom": "^18 || ^19",
    "prettier": "^3.3.3",
    "shx": "^0.3.4",
    "tsup": "^8.5.1",
    "typescript": "^5.6.3"
  },
  "dependencies": {
    "@stianlarsen/copy-to-clipboard": "^1.0.4",
    "shiki": "^1.22.0"
  }
}
